        &:after {
          .transition(opacity, .3s, ease);
          content: '';
          background: url(../../f/i/catalog.png) 0 0 no-repeat;
          left: 0;
          right: 0;
          margin: 0 auto;
          position: absolute;
        }
        &:after {
          opacity: 0;
        }
        &:hover {
          color: #ff5a00;
          &:after {
            opacity: 1;
          }
          &:before {
            opacity: 0;
          }
        }
      }
      &.active a {
        color: #ff5a00;
        &:after {
          opacity: 1;
        }
        &:before {
          opacity: 0;
        }
      }
    }

