  height: 10px;
  margin-left: 10px;
  &:before {
    content: '';
    border-bottom: 1px solid #03a9f4;
    border-right: 1px solid #03a9f4;
    height: 7px;
    position: absolute;
    -ms-transform: rotate(315deg);
    -webkit-transform: rotate(315deg);
    transform: rotate(315deg);
    width: 7px; 
    bottom: 0;
    right: 0;
    top: -3px;
    margin: auto 0;
    .transition(all, .3s, ease);
  }
  &_white:before {
    border-color: #fff;
  }
}
