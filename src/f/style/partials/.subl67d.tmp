  background: #38b549;
  overflow: hidden;
  &__content {
    color: #fff;
    z-index: 10;
    padding: 0 40px 0 20px;
    position: relative;

  }
  &__cell {
    display: table-cell;
    vertical-align: middle;
    position: relative;
    &:last-of-type {
      z-index: 10;
      width: 370px;
      &:before,
      &:after {
        background: #38b549;
        border-radius: 50% 0  0 50%;
        content: "";
        height: 550px;
        left: -70px;
        position: absolute;
        top: -75px;
        width: 500px;
        z-index: 1;
      }
      &:before {
        background: #8fcd40;
        left: -100px;
        z-index: -1;
        width: 500px;
