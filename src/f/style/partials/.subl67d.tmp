    &__holder {
      box-shadow: 0px 10px 10px -10px rgba(212, 212, 212, 0.5);
      background: #fafafa;
      position: -webkit-sticky;
      position: sticky;
      top: 0;
      z-index: 110;
      padding-top: 15px;
      &:before,
      &:after {
        content: '';
        display: table;
      }
    }
    &__item {
      padding-bottom: 35px;
      margin-bottom: 0;
