    overflow: hidden;
    padding-top: 50px;
    padding-bottom: 5px;
  }
  &__title {
    font: 30px/40px 'FregatBold', sans-serif;
    margin-bottom: 10px;
  }
  &__value {
    font-size: 23px;
    margin-bottom: 40px;
    & span {
      font-family: 'FregatBold', sans-serif;
      display: inline-block;
      vertical-align: top;
      margin-left: 10px;
      text-decoration: underline;
      cursor: pointer;
      &.active {
        text-decoration: none;
        cursor: default;
      }
    }
  }
  &__text {
    margin-bottom: 45px;
  }
  &__btn {
    font-size: 19px;
    padding: 10px 40px;
  }
}
.advantage {
  background: #f6f7f7;
  text-align: center;
  &__names,
  &__text {
    & span {
      display: inline-block;
      vertical-align: top;
      width: 33.333333%;
      padding: 0 30px;

    }
  }
  &__names {
    & span {
      font-size: 23px;
    }
  }
  &__text {
    & span {
      font-size: 18px;
      position: relative;
      margin-top: 100px;
      &:before {
        content: '';
        position: absolute;
        left: 0;
        right: 0;
        margin: 0 auto;
        background: url(../../f/i/icons.png) no-repeat;
      }
      &:first-of-type:before {
        width: 74px;
        height: 62px;
        top: -92px;
        background-position: 0 -126px;
      }
      &:nth-of-type(2):before {
        width: 75px;
        height: 57px;
        top: -87px;
        background-position: -89px -139px;
      }
      &:nth-of-type(3):before {
        width: 118px;
        height: 46px;
        top: -76px;
        background-position: -89px -139px;
      }
    }
  }
}
.slides {
  margin-bottom: 0;
  & li {
    margin-bottom: 0;
  }
}
.slider {
  position: relative;
  & ul {
    & li {
      
      &.flex-active-slide {
        z-index: 20;
      }
    }
  }
  & .flexslider-navigation {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    top: 0;
    height: 50px;
    max-width: 1280px;
    margin: auto;
    & a {
      width: 30px;
      height: 30px;
      position: absolute;
      z-index: 2;  
      text-decoration: none;
      top: 0;
      bottom: 0;
      margin: auto 0;
      &.flex-disabled {
        display: none;
      }
      &:after {
        content: '';
        border-bottom: 2px solid #fff;
        border-right: 2px solid #fff;
        height: 22px;
        -ms-transform: rotate(315deg);
        -webkit-transform: rotate(315deg);
        transform: rotate(315deg);
        width: 22px;
        position: absolute;
        top: 0;
        bottom: 0;
        left: 0;
        right: 0;
        margin: auto;
        .transition(border-color, .3s, ease);
      }
      &:hover {
        &:after {
          border-color: #ff5a00;
        }
      }
      &.flex-next {
        right: 30px;
        &:after {
          right: 0;
        }
      }
      &.flex-prev {
        left: 30px;
        &:after {
          -ms-transform: rotate(135deg);
          -webkit-transform: rotate(135deg);
          transform: rotate(135deg);
          left: 0;
        }
      }
    }
  }
  & .flex-control-nav {
    bottom: 15px;
    width: auto;
    left: 0;
    right: 0;
    margin: 0 auto;
    & li {
      &:before {
        content: '';
      }
      & a {
        color: transparent;
        background: #fff;
        width: 14px;
        height: 14px;
        &.flex-active {
          background: #8fcd40;
        }
      }
    }
  }
}

.fancybox-nav {
  display: none;
}
